{
  "name": "@devjaeseo/preactend",
  "version": "0.1.1",
  "description": "implementing the same features of React in a similar way",
  "private": false,
  "keywords": [
    "pretend react",
    "TypeScript"
  ],
  "author": "JaeSeoKim <hello@jaeseokim.dev>",
  "repository": {
    "type": "git",
    "url": "https://github.com/JaeSeoKim/PreactEND.git",
    "directory": "packages/preactend"
  },
  "license": "MIT",
  "main": "./src/index.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    },
    "./jsx-runtime": {
      "import": "./src/jsx-runtime/index.ts",
      "require": "./src/jsx-runtime/index.ts"
    },
    "./dom/client": {
      "import": "./src/dom/client/index.ts",
      "require": "./src/dom/client/index.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "esm"
  ],
  "scripts": {
    "clean": "rimraf dist/ esm/ types/",
    "build": "pnpm clean && pnpm lint && pnpm run \"/^build:(?!analyze).*/\"",
    "build:analyze": "rollup --config --configAnalyze",
    "build:bundle": "rollup -c",
    "build:type": "tsc -p tsconfig.build.json",
    "watch": "rollup -c -w",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "prepack": "pnpm run build"
  },
  "devDependencies": {
    "@configs/babel": "workspace:*",
    "@configs/eslint-config": "workspace:^",
    "@configs/rollup": "workspace:*",
    "@types/node": "^20.9.0",
    "eslint": "^8.53.0",
    "happy-dom": "^12.10.3",
    "rimraf": "^5.0.5",
    "rollup": "^3.29.4",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6"
  },
  "publishConfig": {
    "type": "module",
    "exports": {
      ".": {
        "import": "./esm/index.js",
        "require": "./dist/index.cjs",
        "types": "./types/index.d.ts"
      },
      "./jsx-runtime": {
        "import": "./esm/jsx-runtime/index.js",
        "require": "./dist/jsx-runtime/index.cjs",
        "types": "./types/jsx-runtime/index.d.ts"
      },
      "./dom/client": {
        "import": "./esm/dom/client/index.js",
        "require": "./dist/dom/client/index.cjs",
        "types": "./types/dom/client/index.d.ts"
      },
      "./package.json": "./package.json",
      "registry": "https://npm.pkg.github.com"
    },
    "main": "./dist/index.cjs",
    "module": "./esm/index.js",
    "types": "./types/index.d.ts"
  }
}
